{
  "name": "<%= slugname %>",
  "version": "0.0.0",<% if (props.homepage) { %>
  "homepage": "<%= props.homepage %>",<%}%>
  "authors": [
    "<%= props.authorName %>"
  ],
  "description": "<%= props.description %>",
  "main": "<%= slugname %>.js",
  "keywords": [
    "<%= slugname %>"<% for (var i=0;i<keywords.length;i++) { %>,
    "<%= keywords[i] %>"<% } %>
  ],
  "license": "<%= props.license %>",
  "ignore": [
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ]
}