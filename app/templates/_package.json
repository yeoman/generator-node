{
  "name": "<%= slugname %>",
  "version": "0.0.0",
  "description": "<%= props.description %>",
  "homepage": "<%= props.homepage %>",
  "bugs": "<%= repoUrl %>/issues",
  "author": {
    "name": "<%= props.authorName %>",
    "email": "<%= props.authorEmail %>"<% if (props.authorUrl) { %>,
    "url": "<%= props.authorUrl %>"<% } %>
  },
  "repository": {
    "type": "git",
    "url": "<%= repoUrl %>"
  },
  "licenses": [
    {
      "type": "<%= props.license %>"
    }
  ],
  "devDependencies": {
    "grunt-contrib-jshint": "~0.6.0",
    "grunt-contrib-nodeunit": "~0.2.0",
    "grunt-contrib-watch": "~0.5.0"
  }
}
