{
  "name": "generator-node",
  "version": "2.8.0",
  "description": "Create a Node.js module",
  "homepage": "https://github.com/yeoman/generator-node",
  "author": "Yeoman team",
  "files": [
    "index.js",
    "generators"
  ],
  "main": "index.js",
  "keywords": [
    "yeoman-generator",
    "scaffold",
    "node",
    "module",
    "cli"
  ],
  "devDependencies": {
    "coveralls": "^3.0.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  },
  "repository": "yeoman/generator-node",
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "generator-jest": "^1.7.0",
    "generator-license": "^5.2.0",
    "generator-travis": "^1.9.0",
    "git-remote-origin-url": "^3.0.0",
    "github-username": "^5.0.1",
    "inquirer-npm-name": "^3.0.0",
    "lodash": "^4.17.15",
    "parse-author": "^2.0.0",
    "validate-npm-package-name": "^3.0.0",
    "yeoman-generator": "^7.4.0"
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "engines": {
    "npm": ">= 6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "type": "module"
}
