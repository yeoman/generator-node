{
  "name": "generator-node",
  "version": "2.4.0",
  "description": "Create a Node.js module",
  "homepage": "https://github.com/yeoman/generator-node",
  "author": "Yeoman team",
  "files": [
    "index.js",
    "generators"
  ],
  "main": "index.js",
  "keywords": [
    "yeoman-generator",
    "scaffold",
    "node",
    "module",
    "cli"
  ],
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.20.1",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "lint-staged": "^6.1.1",
    "nsp": "^2.8.0",
    "prettier": "^1.11.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.7.0"
  },
  "repository": "yeoman/generator-node",
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "prepublishOnly": "nsp check",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "generator-jest": "^1.5.1",
    "generator-license": "^5.2.0",
    "generator-travis": "^1.6.0",
    "git-remote-origin-url": "^2.0.0",
    "github-username": "^4.1.0",
    "global-tunnel-ng": "^2.1.1",
    "inquirer-npm-name": "^3.0.0",
    "lodash": "^4.17.5",
    "parse-author": "^2.0.0",
    "validate-npm-package-name": "^3.0.0",
    "yeoman-generator": "^2.0.3"
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "engines": {
    "npm": ">= 4.0.0"
  }
}
