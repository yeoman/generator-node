{
  "name": "generator-node",
  "version": "2.3.0",
  "description": "Create a Node.js module",
  "homepage": "https://github.com/yeoman/generator-node",
  "author": "Yeoman team",
  "files": ["index.js", "generators"],
  "main": "index.js",
  "keywords": ["yeoman-generator", "scaffold", "node", "module", "cli"],
  "devDependencies": {
    "coveralls": "^2.13.1",
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0",
    "lint-staged": "^4.2.1",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "yeoman-assert": "^3.0.0",
    "yeoman-test": "^1.5.1"
  },
  "repository": "yeoman/generator-node",
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "prepublish": "nsp check",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "generator-jest": "^1.3.0",
    "generator-license": "^5.1.0",
    "generator-travis": "^1.5.2",
    "git-remote-origin-url": "^2.0.0",
    "github-username": "^4.0.0",
    "inquirer-npm-name": "^2.0.0",
    "lodash": "^4.17.4",
    "parse-author": "^2.0.0",
    "yeoman-generator": "^1.1.1"
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  }
}
